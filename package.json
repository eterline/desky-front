    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.1",
    "@types/webpack-env": "^1.18.5",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.1",
