{
  "name": "desky-front",
  "version": "1.0.0",
  "description": "Frontend App UI for WEB Panel - Desky",
  "main": "main.js",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.ts --env mode=development",
    "build:prod": "webpack --config webpack.config.ts --env mode=production build=web",
    "test": "test",
    "start": "webpack serve --open --env serverPort=9400",
    "start-proxy": "webpack serve --open --env serverPort=9400 proxy=true",
    "clear": "./clear.sh",
    "clone": "npm run build:prod && ./clone.sh"
  },
  "repository": {
    "type": "git",
    "url": "desky-front"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
